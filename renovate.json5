{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "assignees": [
    "ollegz"
  ],
  "baseBranches": ["main"],
  "timezone": "Europe/Stockholm",
  "rebaseWhen": "behind-base-branch",
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "dependencyDashboardAutoclose": true,
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
}
